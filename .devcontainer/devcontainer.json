{
  "name": "Docker Dev Test",
  "dockerFile": "Dockerfile",
  "appPort": [
    3000,
    3001
  ],
  "runArgs": [
    "-u",
    "node"
  ],
  "settings": {
    "workbench.colorTheme": "Default Dark+",
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "postCreateCommand": "npm install",
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.pretter-vscode"
  ]
}